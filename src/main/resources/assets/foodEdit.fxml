<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.WhaleTech.Controllers.FoodController">
   <children>
      <GridPane alignment="CENTER" vgap="10.0">
         <children>
            <Label text="Navn:" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
               <font>
                  <Font size="15.0" />
               </font>
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin>
            </Label>
            <Label text="Status:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="CENTER">
               <font>
                  <Font size="15.0" />
               </font>
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin>
            </Label>
            <Label text="Symptomer:" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER">
               <font>
                  <Font size="15.0" />
               </font>
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin>
            </Label>
            <TextField fx:id="nameField" prefHeight="25.0" prefWidth="120.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" promptText="&amp;,$ and : not allowed!"/>
            <ChoiceBox fx:id="stateChoice" prefWidth="120.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
            <MenuButton fx:id="selectedSymptoms" mnemonicParsing="false" prefWidth="120.0" text="Symptomes" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
            <Label text="Kategori:" GridPane.halignment="RIGHT" GridPane.rowIndex="3">
               <font>
                  <Font size="15.0" />
               </font>
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin>
            </Label>
            <ChoiceBox fx:id="categories" prefWidth="150.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="123.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="178.0" minWidth="10.0" prefWidth="174.0" />
         </columnConstraints>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <rowConstraints>
            <RowConstraints maxHeight="260.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="260.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="336.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="336.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
      <CheckBox fx:id="isCategory" alignment="CENTER" mnemonicParsing="false" text="Er Kategori?">
         <font>
            <Font size="15.0" />
         </font>
      </CheckBox>
      <Separator prefHeight="9.0" prefWidth="307.0" />
      <HBox spacing="5.0">
         <children>
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="addBtn" mnemonicParsing="false" text="Legg Til" />
            <Region prefWidth="2.0" />
            <Button fx:id="closeBtn" mnemonicParsing="false" text="Lukk" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin></HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
